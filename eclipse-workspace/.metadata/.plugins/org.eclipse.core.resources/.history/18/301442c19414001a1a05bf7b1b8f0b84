
public class Preference {
	private int quietTime, music, reading, chatting;
	
	Preference(int theQT, int theMusic, int theReading, int theChatting){
		quietTime = theQT;
		music = theMusic;
		reading = theReading;
		chatting = theChatting;
	}
	
	public int getQT() {
		return quietTime;
	}
	
	public int getMusic() {
		return music;
	}
	
	public int getChatting() {
		return chatting;
	}
}
